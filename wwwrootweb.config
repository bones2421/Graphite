<rewrite>
  <rules>
    <rule name="Redirect to custom domain" stopProcessing="true">
      <match url="(.*)" />
      <conditions>
        <add input="{HTTP_HOST}" pattern="^graphite.azurewebsites.net$" />
      </conditions>
      <action type="Redirect" url="https://www.graphiteplaypen.com/{R:1}" redirectType="Permanent" />
    </rule>
  </rules>
</rewrite>

